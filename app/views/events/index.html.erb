<%= link_to "Back to Home", root_path %>
<%= link_to "Create something!", new_event_path %>
<h1>Projects and Activities</h1>

<% @events.each do |event| %>
  <ul>
    <li><%= event.title %></li>
    <%= cl_image_tag event.image.key, height: 300, width: 400, crop: :fill %>

    <li><%= event.description %></li>
    <li>Category: <%= event.category.category %></li>
    <li>Date: <%= event.date %></li>
    <li>Time: <%= event.time %></li>
    <li>Location: <%= event.location %></li>
    <li>Created by: <%= event.user.email %></li>
    <%= link_to "Show Event", event_path(event) %>
  </ul>

<% end %>

